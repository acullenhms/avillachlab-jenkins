<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This job will download / update the gen3-client&#xd;
&#xd;
It will also configure a default profile for gen3-client to use.&#xd;
&#xd;
You will need an api-key from gen3 in order to run this job.</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.PasswordParameterDefinition>
          <name>__CREDIENTIALS__</name>
          <description>Paste the gen3 api credientials.json file here</description>
          <defaultValue>{}</defaultValue>
        </hudson.model.PasswordParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>__API_END_POINT__</name>
          <description></description>
          <defaultValue>https://gen3.biodatacatalyst.nhlbi.nih.gov/</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>curl https://api.github.com/repos/uc-cdis/cdis-data-client/releases/latest | grep browser_download_url.*linux |  cut -d &apos;&quot;&apos; -f 4 | wget -qi -

unzip -o dataclient_linux.zip

cp gen3-client /usr/local/bin/ | cp gen3-client /usr/bin/

set +x

echo ${__CREDIENTIALS__} &gt; credientials.json 

set -x

gen3-client configure --profile=jenkins --cred=credientials.json --apiendpoint=${__API_END_POINT__}

rm -rf credientials.json  
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>